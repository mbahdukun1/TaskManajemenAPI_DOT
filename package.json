{
  "name": "mini-project-pt.dot",
  "version": "0.1.0",
  "description": "Mini Project - Task Management API (NestJS + PostgreSQL + TypeORM + Redis)",
  "private": true,
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main.js",
    "build": "nest build",
    "lint": "eslint",
    "typeorm:generate": "ts-node ./node_modules/typeorm/cli.js -d typeorm.config.ts migration:generate src/migrations/InitSchema",
    "typeorm:create": "ts-node ./node_modules/typeorm/cli.js -d typeorm.config.ts migration:create src/migrations/ManualEdit",
    "typeorm:run": "ts-node ./node_modules/typeorm/cli.js -d typeorm.config.ts migration:run",
    "typeorm:revert": "ts-node ./node_modules/typeorm/cli.js -d typeorm.config.ts migration:revert"
  },
  "dependencies": {
    "@bull-board/api": "^6.12.0",
    "@bull-board/express": "^6.12.0",
    "@nestjs/bullmq": "^11.0.3",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^10.4.20",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.1.15",
    "@nestjs/typeorm": "^10.0.0",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.56.9",
    "cache-manager": "^7.1.1",
    "cache-manager-ioredis-yet": "^1.2.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.4.5",
    "ioredis": "^5.4.1",
    "nestjs": "^0.0.1",
    "nestjs-pino": "^4.4.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.5",
    "pino": "^9.8.0",
    "pino-pretty": "^13.1.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.3",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "@types/passport-jwt": "^4.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  }
}
